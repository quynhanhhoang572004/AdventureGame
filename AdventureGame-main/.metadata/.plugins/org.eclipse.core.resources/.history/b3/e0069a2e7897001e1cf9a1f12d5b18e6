package object;

import java.io.File;
import java.io.IOException;
import javax.imageio.ImageIO;

public class OBJ_Boots extends SuperObject {
	    public OBJ_Boots () {
	        name = "Boots";
	        try {
	            image = ImageIO.read(new File("res/objects/boots.png")); // Dùng getResourceAsStream khi chạy code sẽ bị lỗi 
	        } catch (IOException e) {
	            e.printStackTrace();
	        }
	    }
	}


}
